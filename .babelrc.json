{
  "presets": [
    ["@babel/env", { "modules": false }],
    ["@babel/typescript", { "jsxFactory": "h" }]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx",
    "@babel/plugin-proposal-class-properties",
    [
      "const-enum",
      {
        "transform": "constObject"
      }
    ]
  ]
}
